            "providers": [
                "Crud\\CrudGenerator\\CrudGeneratorServiceProvider"
            ],
            "aliases": {
                "CrudGenerator": "Crud\\CrudGenerator\\Facades\\CrudGenerator"
            }
        }
    }
}
